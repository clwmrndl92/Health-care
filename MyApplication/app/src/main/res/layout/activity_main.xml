<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/main_health_buton"
        android:text="healh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="ChangeHealthActivity"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@id/main_memo_buton"
        app:layout_constraintLeft_toLeftOf="parent"></Button>

    <Button
        android:id="@+id/main_memo_buton"
        android:text="memo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="ChangeMemoActivity"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@id/main_health_buton"></Button>

</androidx.constraintlayout.widget.ConstraintLayout>